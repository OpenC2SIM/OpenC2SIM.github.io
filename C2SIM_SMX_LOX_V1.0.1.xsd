      </xs:sequence>
   </xs:complexType>
   <xs:element name="TaskRequestContent" type="TaskRequestContentType"/>
   <xs:complexType name="TaskStatusType">
      <xs:annotation>
         <xs:documentation>The status of a task, provided before, during or after a task is executed.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TaskStatus</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="ReportContentGroup"/>
         <xs:element ref="CurrentTask" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="TaskStatusCode" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
   <xs:element name="TaskStatus" type="TaskStatusType"/>
   <xs:simpleType name="TaskStatusCodeType">
      <xs:annotation>
         <xs:documentation>An enumeration of possible status values for a task.</xs:documentation>

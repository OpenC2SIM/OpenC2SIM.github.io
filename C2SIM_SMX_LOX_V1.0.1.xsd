      <xs:annotation>
         <xs:documentation>Class of all concepts for the content of messages in the core C2SIM data model.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#C2SIMContent</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="AbstractObject"/>
         <xs:element ref="Action"/>
         <xs:element ref="Code"/>
         <xs:element ref="Entity"/>
         <xs:element ref="EntityDescriptor"/>
         <xs:element ref="EntityState"/>
         <xs:element ref="EntityType"/>
         <xs:element ref="Observation"/>
         <xs:element ref="PhysicalConcept"/>
         <xs:element ref="PlanPhase"/>
         <xs:element ref="PlanPhaseTrigger"/>
         <xs:element ref="Relationship"/>
         <xs:element ref="Resource"/>
         <xs:element ref="RuleOfEngagement"/>
      </xs:choice>
   </xs:complexType>
   <xs:element name="C2SIMContent" type="C2SIMContentType"/>

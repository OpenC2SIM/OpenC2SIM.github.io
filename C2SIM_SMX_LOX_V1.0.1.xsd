      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="TemporalConceptType">
      <xs:annotation>
         <xs:documentation>Time concepts include instants in time, durations of time, and intervals between two times.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TemporalConcept</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="Duration"/>
         <xs:element ref="IntervalTime"/>
         <xs:element ref="TimeInstant"/>
      </xs:choice>
   </xs:complexType>
   <xs:group name="TimeInstantGroup">
      <xs:annotation>
         <xs:documentation>A specific time expressed in seconds, i.e. with no date. The generator and user of this time must understand a common reference point, for example the start of the scenario.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TimeInstant</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="Name" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
   </xs:group>
   <xs:complexType name="TimeInstantType">
      <xs:annotation>
         <xs:documentation>A specific time expressed in seconds, i.e. with no date. The generator and user of this time must understand a common reference point, for example the start of the scenario.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#TimeInstant</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="DateTime"/>
         <xs:element ref="RelativeTime"/>
         <xs:element ref="SimulationTime"/>
      </xs:choice>
   </xs:complexType>
   <xs:simpleType name="TimeReferenceCodeType">
      <xs:annotation>

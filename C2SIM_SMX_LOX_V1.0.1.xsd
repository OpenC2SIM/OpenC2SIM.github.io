   <xs:annotation>
      <xs:documentation xml:lang="en">********************************************************************************* COMPLEX TYPES AND MODEL GROUPS DERIVED FROM ONTOLOGY CLASSES *********************************************************************************</xs:documentation>
   </xs:annotation>
   <xs:group name="AbstractObjectGroup">
      <xs:annotation>
         <xs:documentation>An AbstractObject is an object in the system that does not have the full characteristics of an entity. For example, it does not have a location or an extent. It does however have a unique identifier.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AbstractObject</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="Name" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="UUID" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:group>
   <xs:complexType name="AbstractObjectType">
      <xs:annotation>
         <xs:documentation>An AbstractObject is an object in the system that does not have the full characteristics of an entity. For example, it does not have a location or an extent. It does however have a unique identifier.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AbstractObject</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="AbstractOrganization"/>
         <xs:element ref="CommunicationNetwork"/>
         <xs:element ref="ForceSide"/>
         <xs:element ref="Overlay"/>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="AbstractOrganizationType">
      <xs:annotation>
         <xs:documentation>A group that an actor can be affiliated with. Does not perform any actions and does not have a specific location.
Subclasses of this class can be created for country, religion, etc. Each of these will have defined instances (one for each country, etc.)</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AbstractOrganization</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="AbstractObjectGroup"/>
         <xs:element ref="CountryCode" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="EthnicGroupCode" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="OrganizationTypeCode" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="ReligionCode" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
   <xs:element name="AbstractOrganization" type="AbstractOrganizationType"/>
   <xs:simpleType name="AcknowledgeTypeCodeType">
      <xs:annotation>
         <xs:documentation>Enumeration of different types of acknowledgements.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AcknowledgeTypeCode</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="ACKFAIL"/>
         <xs:enumeration value="ACKNOTEXEC"/>
         <xs:enumeration value="ACKNOTRECGNZ"/>
         <xs:enumeration value="ACKNOTUNDSTD"/>
         <xs:enumeration value="ACKRCVD"/>
         <xs:enumeration value="ACKREQDEN"/>
         <xs:enumeration value="ACKREQGRT"/>
         <xs:enumeration value="ACKSUCC"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="AcknowledgementBodyType">
      <xs:annotation>
         <xs:documentation>An acknowledgement from one Actor to another that a message was received.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AcknowledgementBody</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:group ref="DomainMessageBodyGroup"/>
         <xs:element ref="AcknowledgeTypeCode" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
   <xs:element name="AcknowledgementBody" type="AcknowledgementBodyType"/>
   <xs:group name="ActionGroup">
      <xs:annotation>
         <xs:documentation>Any occurrence that occurs at a moment in time or over a period of time.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#Action</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="ActionTemporalRelationship"
                     minOccurs="0"
                     maxOccurs="unbounded"/>
         <xs:element ref="Location" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="MapGraphicID" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="Name" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="UUID" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:group>
   <xs:complexType name="ActionType">
      <xs:annotation>
         <xs:documentation>Any occurrence that occurs at a moment in time or over a period of time.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#Action</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="Event"/>
         <xs:element ref="Task"/>
      </xs:choice>
   </xs:complexType>
   <xs:complexType name="ActionCodeType">
      <xs:annotation>
         <xs:documentation>An enumeration describing what the action is.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ActionCode</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="EventCode"/>
         <xs:element ref="TaskActionCode"/>
      </xs:choice>
   </xs:complexType>
   <xs:simpleType name="ActionTemporalAssociationCodeType">
      <xs:annotation>
         <xs:documentation>An enumeration describing the temporal relationship between two actions.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ActionTemporalAssociationCode</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="ENDEND"/>
         <xs:enumeration value="ENDENE"/>
         <xs:enumeration value="ENDENL"/>
         <xs:enumeration value="ENDSNE"/>
         <xs:enumeration value="ENDSNL"/>
         <xs:enumeration value="ENDSTR"/>
         <xs:enumeration value="SAEAST"/>
         <xs:enumeration value="SAENDO"/>
         <xs:enumeration value="SASTEA"/>
         <xs:enumeration value="SBEAST"/>
         <xs:enumeration value="SDUREA"/>
         <xs:enumeration value="SDUREB"/>
         <xs:enumeration value="STREND"/>
         <xs:enumeration value="STRENE"/>
         <xs:enumeration value="STRENL"/>
         <xs:enumeration value="STRSNE"/>
         <xs:enumeration value="STRSNL"/>
         <xs:enumeration value="STRSTR"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="ActionTemporalRelationshipType">
      <xs:annotation>
         <xs:documentation>A temporal relationship between two actions. Some relationships involve a time duration.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ActionTemporalRelationship</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="ActionTemporalAssociationCode" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="Duration" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="TemporalAssociationWithAction" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
   <xs:group name="ActorEntityGroup">
      <xs:annotation>
         <xs:documentation>An ActorEntity is an entity that can send/receive orders and reports and/or perform tasks.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ActorEntity</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="CurrentTask" minOccurs="0" maxOccurs="unbounded"/>
         <xs:element ref="EntityDescriptor" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="Resource" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
   </xs:group>
   <xs:complexType name="ActorEntityType">
      <xs:annotation>
         <xs:documentation>An ActorEntity is an entity that can send/receive orders and reports and/or perform tasks.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#ActorEntity</xs:documentation>
      </xs:annotation>
      <xs:choice>
         <xs:element ref="CollectiveEntity"/>
         <xs:element ref="Person"/>
         <xs:element ref="Platform"/>
      </xs:choice>
   </xs:complexType>
   <xs:element name="ActorEntity" type="ActorEntityType"/>
   <xs:complexType name="AllegianceRelationshipType">
      <xs:annotation>
         <xs:documentation>This is a relationship of friendly, hostile, or neutral toward an actor entity. It is part of the definition of the hasAllegianceTo property of an EntityDescriptor.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AllegianceRelationship</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="ActorReference" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="AllegianceRelationshipCode" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="AllegianceRelationshipCodeType">
      <xs:annotation>
         <xs:documentation>This code defines the types of allegiance in an AllegianceRelationship.</xs:documentation>
         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#AllegianceRelationshipCode</xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="FriendlyTo"/>
         <xs:enumeration value="HostileTo"/>
         <xs:enumeration value="NeutralTo"/>
         <xs:enumeration value="UnkownAllegianceTo"/>
      </xs:restriction>
   </xs:simpleType>

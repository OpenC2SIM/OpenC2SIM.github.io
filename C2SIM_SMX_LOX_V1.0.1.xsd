         <xs:documentation>http://www.sisostds.org/ontologies/C2SIM#C2SIMHeader</xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element ref="CommunicativeActTypeCode" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="ConversationID" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="FromSendingSystem" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="InReplyToMessageID" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="MessageID" minOccurs="1" maxOccurs="1"/>
         <xs:element ref="Protocol"/>
         <xs:element ref="ProtocolVersion"/>
         <xs:element ref="ReplyToSystem" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="SecurityClassificationCode" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="SendingTime" minOccurs="0" maxOccurs="1"/>
         <xs:element ref="ToReceivingSystem" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
   </xs:complexType>
